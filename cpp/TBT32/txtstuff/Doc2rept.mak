$(DST)\TRANSITR.TBL: $(SRC)\TRANSITR$(EXT) $(SRC)\TRANSAFT$(EXT)
	+del $(DST)\TRANSITR.TBL
	buildrpt $(SRC)\TRANSITR$(EXT) $(DST)\TRANSITR.TBL X $(FMT) T N
	buildrpt $(SRC)\TRANSAFT$(EXT) $(DST)\TRANSITR.TBL X $(FMT) T X

$(DST)\COMPOS.TBL: $(SRC)\COMPOS$(EXT)
	+del $(DST)\COMPOS.TBL
	buildrpt $(SRC)\COMPOS$(EXT) $(DST)\COMPOS.TBL X $(FMT) T N

$(DST)\COMPAT.TBL: $(SRC)\COMPAT$(EXT)
	+del $(DST)\COMPAT.TBL
