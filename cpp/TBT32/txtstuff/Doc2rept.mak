
# BUILD REPORTS

.SUFFIXES: $(EXT) .TBL .BSM .SYM

all: REPORT.BSM STRTABLE.BSM $(DST)\GLOBAL.TBL $(DST)\helio.TBL \
	$(DST)\RETURN.TBL $(DST)\FIXEDST.TBL $(DST)\ARABIC.TBL \
	$(DST)\NUMERIC.TBL $(DST)\PROGRESS.TBL $(DST)\TRANSITR.TBL \
	$(DST)\COMPOS.TBL $(DST)\COMPAT.TBL $(DST)\NATAL.TBL \
	$(DST)\SUMMARY.TBL $(DST)\COSUMMR.TBL $(DST)\COTRANS.TBL \
	$(DST)\PROBLEM.TBL

REPORT.BSM : REPORT.SYM
	MKRBSR REPORT.BSM REPORT.SYM

STRTABLE.BSM : STRING.TAB CHAR.TAB
    MKSTRB STRING.TAB CHAR.TAB

$(DST)\GLOBAL.TBL: $(SRC)\gender$(EXT) $(SRC)\chhead$(EXT) \
		$(SRC)\STRING$(EXT) $(SRC)\monthstr$(EXT)
	+del $(DST)\GLOBAL.TBL
	buildrpt $(SRC)\gender$(EXT) $(DST)\GLOBAL.TBL X $(FMT) T X
	buildrpt $(SRC)\chhead$(EXT) $(DST)\GLOBAL.TBL X $(FMT) T N
	buildrpt $(SRC)\STRING$(EXT) $(DST)\GLOBAL.TBL X $(FMT) T X
	buildrpt $(SRC)\monthstr$(EXT) $(DST)\GLOBAL.TBL X $(FMT) T X

$(DST)\helio.TBL: $(SRC)\helio$(EXT)
	+del $(DST)\HELIO.TBL
	buildrpt $(SRC)\helio$(EXT) $(DST)\helio.TBL X $(FMT) T N

$(DST)\PROBLEM.TBL: $(SRC)\PROBLEM$(EXT) $(SRC)\PROBRES$(EXT)
	+del $(DST)\PROBLEM.TBL
	buildrpt $(SRC)\PROBLEM$(EXT) $(DST)\PROBLEM.TBL X $(FMT) T N
	buildrpt $(SRC)\PROBRES$(EXT) $(DST)\PROBLEM.TBL X $(FMT) T X

$(DST)\RETURN.TBL: $(SRC)\RETURN$(EXT)
	+del $(DST)\RETURN.TBL
	buildrpt $(SRC)\RETURN$(EXT) $(DST)\RETURN.TBL X $(FMT) T N

$(DST)\FIXEDST.TBL: $(SRC)\FIXEDST$(EXT) $(SRC)\FIXDNAMS$(EXT)
	+del $(DST)\FIXEDST.TBL
	buildrpt $(SRC)\FIXEDST$(EXT) $(DST)\FIXEDST.TBL X $(FMT) T N
	buildrpt $(SRC)\FIXDNAMS$(EXT) $(DST)\FIXEDSt.TBL X $(FMT) T X

$(DST)\ARABIC.TBL: $(SRC)\ARABIC$(EXT) $(SRC)\ARABNAM$(EXT)
	+del $(DST)\ARABIC.TBL
	buildrpt $(SRC)\ARABIC$(EXT) $(DST)\ARABIC.TBL X $(FMT) T N
	buildrpt $(SRC)\ARABNAM$(EXT) $(DST)\ARABIC.TBL X $(FMT) T X

$(DST)\NUMERIC.TBL: $(SRC)\NUMERIC$(EXT)
	+del $(DST)\NUMERIC.TBL
